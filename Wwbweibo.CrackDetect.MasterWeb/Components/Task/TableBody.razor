@using Wwbweibo.CrackDetect.MasterWeb.Components.Task
@{

    foreach (var item in TableData)
    {
        if (item.Value > 0)
        {

            Index += 1;
            <TableRow Index="Index" TaskId="@item.Key" TaskCount="item.Value" />
        }
    }

}


@code {
    private int Index = 0;
    [Parameter]
    public Dictionary<string, int> TableData { get; set; }

    [Parameter]
    public EventCallback<Dictionary<string, int>> TableDataChanged { get; set; }

}
